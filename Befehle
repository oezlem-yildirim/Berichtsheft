\newcommand{\Titelzeile}[3]{
	\centering

	\begin{tabularx}{\textwidth}{lrX}
		\multirow{4}{10cm}{\includegraphics[width=50mm]{Bilder/Jung_logo.png}}
			& Name: & \Name \\\cline{3-3}
    		& Abteilung: & \Abteilung \\\cline{3-3}
    		& Monat: & #1 \\\cline{3-3}
    		& Ausbildungsjahr: & #2 \\\cline{3-3}
    	\hline
	\end{tabularx}

	\begin{flushleft}
		\huge Ausbildungsnachweis Nr. #3
	\end{flushleft}
}

\newcommand{\Unterschrift}{
